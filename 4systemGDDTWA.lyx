\begin_body

\begin_layout Title
GDDTWA approach to a 4-level system dynamics
\end_layout

\begin_layout Standard
For determining the Wigner discrete function using the approach from Bihui
 et al.
 for our 4-level scheme we consider the set of operators of the form:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\varLambda_{10,i}^{x} & =\frac{1}{\sqrt{2}}\left(\left|1\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 1\right|_{i}\right),\\
\varLambda_{20,i}^{x} & =\frac{1}{\sqrt{2}}\left(\left|2\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 2\right|_{i}\right),\\
\varLambda_{30,i}^{x} & =\frac{1}{\sqrt{2}}\left(\left|3\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 3\right|_{i}\right),\\
\varLambda_{21,i}^{x} & =\frac{1}{\sqrt{2}}\left(\left|2\right\rangle \!\!\left\langle 1\right|_{i}+\left|1\right\rangle \!\!\left\langle 2\right|_{i}\right),\\
\varLambda_{31,i}^{x} & =\frac{1}{\sqrt{2}}\left(\left|3\right\rangle \!\!\left\langle 1\right|_{i}+\left|1\right\rangle \!\!\left\langle 3\right|_{i}\right),\\
\varLambda_{32,i}^{x} & =\frac{1}{\sqrt{2}}\left(\left|3\right\rangle \!\!\left\langle 2\right|_{i}+\left|2\right\rangle \!\!\left\langle 3\right|_{i}\right),\\
\varLambda_{10,i}^{y} & =\frac{1}{\sqrt{2}{\rm i}}\left(\left|0\right\rangle \!\!\left\langle 1\right|_{i}-\left|1\right\rangle \!\!\left\langle 0\right|_{i}\right),\\
\varLambda_{20,i}^{y} & =\frac{1}{\sqrt{2}{\rm i}}\left(\left|0\right\rangle \!\!\left\langle 2\right|_{i}-\left|2\right\rangle \!\!\left\langle 0\right|_{i}\right),\\
\varLambda_{30,i}^{y} & =\frac{1}{\sqrt{2}{\rm i}}\left(\left|0\right\rangle \!\!\left\langle 3\right|_{i}-\left|3\right\rangle \!\!\left\langle 0\right|_{i}\right),\\
\varLambda_{21,i}^{y} & =\frac{1}{\sqrt{2}{\rm i}}\left(\left|1\right\rangle \!\!\left\langle 2\right|_{i}-\left|2\right\rangle \!\!\left\langle 1\right|_{i}\right),\\
\varLambda_{31,i}^{y} & =\frac{1}{\sqrt{2}{\rm i}}\left(\left|1\right\rangle \!\!\left\langle 3\right|_{i}-\left|3\right\rangle \!\!\left\langle 1\right|_{i}\right),\\
\varLambda_{32,i}^{y} & =\frac{1}{\sqrt{2}{\rm i}}\left(\left|2\right\rangle \!\!\left\langle 3\right|_{i}-\left|3\right\rangle \!\!\left\langle 2\right|_{i}\right),\\
\varLambda_{1,i}^{z} & =\frac{1}{\sqrt{2}}\left(\left|0\right\rangle \!\!\left\langle 0\right|_{i}-\left|1\right\rangle \!\!\left\langle 1\right|_{i}\right),
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\varLambda_{2,i}^{z} & =\frac{1}{\sqrt{6}}\left(\left|0\right\rangle \!\!\left\langle 0\right|_{i}+\left|1\right\rangle \!\!\left\langle 1\right|_{i}-2\left|2\right\rangle \!\!\left\langle 2\right|_{i}\right),\\
\varLambda_{3,i}^{z} & =\frac{1}{\sqrt{12}}\left(\left|0\right\rangle \!\!\left\langle 0\right|_{i}+\left|1\right\rangle \!\!\left\langle 1\right|_{i}+\left|2\right\rangle \!\!\left\langle 2\right|_{i}-3\left|3\right\rangle \!\!\left\langle 3\right|_{i}\right).
\end{align}

\end_inset


\end_layout

\begin_layout Standard
We will start to consider the state of the form 
\begin_inset Formula $\left|0\right\rangle $
\end_inset

, this state has the following description: 
\begin_inset Formula $\left(\lambda,p_{\lambda}\right)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\lambda_{0,1,2} & =\left\{ \left(\frac{1}{\sqrt{2}},\frac{1}{2}\right),\left(-\frac{1}{\sqrt{2}},\frac{1}{2}\right)\right\} ,\\
\lambda_{3,4,5} & =\left\{ \left(0,1\right)\right\} ,\\
\lambda_{6,7,8} & =\left\{ \left(\frac{1}{\sqrt{2}},\frac{1}{2}\right),\left(-\frac{1}{\sqrt{2}},\frac{1}{2}\right)\right\} ,\\
\lambda_{9,10,11} & =\left\{ \left(0,1\right)\right\} ,\\
\lambda_{12} & =\left\{ \left(\frac{1}{\sqrt{2}},1\right)\right\} ,\\
\lambda_{13} & =\left\{ \left(\frac{1}{\sqrt{6}},1\right)\right\} ,\\
\lambda_{14} & =\left\{ \left(\frac{1}{\sqrt{12}},1\right)\right\} .
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Now we perform this estimation with 
\begin_inset Formula $\frac{\left|0\right\rangle +\left|1\right\rangle }{\sqrt{2}}$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\lambda_{0} & =\left\{ \left(\frac{1}{\sqrt{2}},1\right)\right\} ,\\
\lambda_{1,2,3,4} & =\left\{ \left(\frac{1}{\sqrt{2}},\frac{1}{4}\right),\left(-\frac{1}{\sqrt{2}},\frac{1}{4}\right),\left(0,\frac{1}{2}\right)\right\} ,\\
\lambda_{5} & =\left\{ \left(0,1\right)\right\} ,\\
\lambda_{6} & =\left\{ \left(\frac{1}{\sqrt{2}},\frac{1}{2}\right),\left(-\frac{1}{\sqrt{2}},\frac{1}{2}\right)\right\} ,\\
\lambda_{7,8,9,10} & =\left\{ \left(\frac{1}{\sqrt{2}},\frac{1}{4}\right),\left(-\frac{1}{\sqrt{2}},\frac{1}{4}\right),\left(0,\frac{1}{2}\right)\right\} ,\\
\lambda_{11} & =\left\{ \left(0,1\right)\right\} ,\\
\lambda_{12} & =\left\{ \left(\frac{1}{\sqrt{2}},\frac{1}{2}\right),\left(-\frac{1}{\sqrt{2}},\frac{1}{2}\right)\right\} ,\\
\lambda_{13} & =\left\{ \left(\frac{1}{\sqrt{6}},\frac{1}{2}\right),\left(\frac{1}{\sqrt{6}},\frac{1}{2}\right)\right\} ,\\
\lambda_{14} & =\left\{ \left(\frac{1}{\sqrt{12}},\frac{1}{2}\right),\left(\frac{1}{\sqrt{12}},\frac{1}{2}\right)\right\} .
\end{align}

\end_inset


\end_layout

\begin_layout Standard
We retake the hamiltonian of the form:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
H & =g_{{\rm R}}\sum_{i}\left(a_{{\rm R}}\left|3\right\rangle \!\!\left\langle 0\right|_{i}+a_{{\rm R}}^{\dagger}\left|0\right\rangle \!\!\left\langle 3\right|_{i}\right)-\Delta_{{\rm R}}a_{{\rm R}}^{\dagger}a_{{\rm R}}+\frac{\Omega_{{\rm R}}}{2}\sum_{i}\left(\left|3\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 3\right|_{i}\right)\\
 & \,\,\,\,\,+g_{{\rm L}}\sum_{i}\left(a_{{\rm L}}\left|2\right\rangle \!\!\left\langle 1\right|_{i}+a_{{\rm L}}^{\dagger}\left|1\right\rangle \!\!\left\langle 2\right|_{i}\right)-\Delta_{{\rm L}}a_{{\rm L}}^{\dagger}a_{{\rm L}}+\frac{\Omega_{{\rm L}}}{2}\sum_{i}\left(\left|2\right\rangle \!\!\left\langle 1\right|_{i}+\left|1\right\rangle \!\!\left\langle 2\right|_{i}\right).
\end{align}

\end_inset


\end_layout

\begin_layout Standard
So we have:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\sqrt{2}\varLambda_{10,i}^{x} & =\left|0\right\rangle \!\!\left\langle 1\right|_{i}+\left|1\right\rangle \!\!\left\langle 0\right|_{i}\\
\sqrt{2}{\rm i}\varLambda_{10,i}^{y} & =\left|0\right\rangle \!\!\left\langle 1\right|_{i}-\left|1\right\rangle \!\!\left\langle 0\right|_{i},\\
\left|0\right\rangle \!\!\left\langle 1\right|_{i} & =\frac{\varLambda_{10,i}^{x}+{\rm i}\varLambda_{10,i}^{y}}{\sqrt{2}},\\
\left|1\right\rangle \!\!\left\langle 0\right|_{i} & =\frac{\varLambda_{10,i}^{x}-{\rm i}\varLambda_{10,i}^{y}}{\sqrt{2}}.
\end{align}

\end_inset


\end_layout

\begin_layout Standard
In general for 
\begin_inset Formula $a<b$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\left|a\right\rangle \!\!\left\langle b\right|_{i} & =\frac{\varLambda_{ba,i}^{x}+{\rm i}\varLambda_{ba,i}^{y}}{\sqrt{2}},\\
\left|b\right\rangle \!\!\left\langle a\right|_{i} & =\frac{\varLambda_{ba,i}^{x}-{\rm i}\varLambda_{ba,i}^{y}}{\sqrt{2}}.
\end{align}

\end_inset


\end_layout

\begin_layout Standard
So, in particular:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\left|3\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 3\right|_{i} & =\sqrt{2}\varLambda_{30,i}^{x},\\
\sum_{i}\varLambda_{30,i}^{x} & =\varLambda_{30}^{x},\\
\left|2\right\rangle \!\!\left\langle 1\right|_{i}+\left|1\right\rangle \!\!\left\langle 2\right|_{i} & =\sqrt{2}\varLambda_{21,i}^{x},\\
\sum_{i}\varLambda_{21,i}^{x} & =\varLambda_{21}^{x}.
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
H & =g_{{\rm R}}\sum_{i}\left(a_{{\rm R}}\frac{\varLambda_{30,i}^{x}+{\rm i}\varLambda_{30,i}^{y}}{\sqrt{2}}+a_{{\rm R}}^{\dagger}\frac{\varLambda_{30,i}^{x}-{\rm i}\varLambda_{30,i}^{y}}{\sqrt{2}}\right)-\Delta_{{\rm R}}a_{{\rm R}}^{\dagger}a_{{\rm R}}+\frac{\Omega_{{\rm R}}}{\sqrt{2}}\varLambda_{30}^{x}\\
 & \,\,\,\,\,+g_{{\rm L}}\sum_{i}\left(a_{{\rm L}}\frac{\varLambda_{21,i}^{x}+{\rm i}\varLambda_{21,i}^{y}}{\sqrt{2}}+a_{{\rm L}}^{\dagger}\frac{\varLambda_{21,i}^{x}-{\rm i}\varLambda_{21,i}^{y}}{\sqrt{2}}\right)-\Delta_{{\rm L}}a_{{\rm L}}^{\dagger}a_{{\rm L}}+\frac{\Omega_{{\rm L}}}{\sqrt{2}}\varLambda_{21}^{x}.
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Rearranging:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
H & =\frac{g_{{\rm R}}}{\sqrt{2}}\sum_{i}\left(a_{{\rm R}}\left(\varLambda_{30,i}^{x}+{\rm i}\varLambda_{30,i}^{y}\right)+a_{{\rm R}}^{\dagger}\left(\varLambda_{30,i}^{x}-{\rm i}\varLambda_{30,i}^{y}\right)\right)-\Delta_{{\rm R}}a_{{\rm R}}^{\dagger}a_{{\rm R}}+\frac{\Omega_{{\rm R}}}{\sqrt{2}}\varLambda_{30}^{x}\\
 & \,\,\,\,\,+\frac{g_{{\rm L}}}{\sqrt{2}}\sum_{i}\left(a_{{\rm L}}\left(\varLambda_{21,i}^{x}+{\rm i}\varLambda_{21,i}^{y}\right)+a_{{\rm L}}^{\dagger}\left(\varLambda_{21,i}^{x}-{\rm i}\varLambda_{21,i}^{y}\right)\right)-\Delta_{{\rm L}}a_{{\rm L}}^{\dagger}a_{{\rm L}}+\frac{\Omega_{{\rm L}}}{\sqrt{2}}\varLambda_{21}^{x}.
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Just for computing properly:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
H & =g_{{\rm R}}\sum_{i}\left(a_{{\rm R}}\left|3\right\rangle \!\!\left\langle 0\right|_{i}+a_{{\rm R}}^{\dagger}\left|0\right\rangle \!\!\left\langle 3\right|_{i}\right)-\Delta_{{\rm R}}a_{{\rm R}}^{\dagger}a_{{\rm R}}+\frac{\Omega_{{\rm R}}}{\sqrt{2}}\varLambda_{30}^{x}\\
 & \,\,\,\,\,+g_{{\rm L}}\sum_{i}\left(a_{{\rm L}}\left|2\right\rangle \!\!\left\langle 1\right|_{i}+a_{{\rm L}}^{\dagger}\left|1\right\rangle \!\!\left\langle 2\right|_{i}\right)-\Delta_{{\rm L}}a_{{\rm L}}^{\dagger}a_{{\rm L}}+\frac{\Omega_{{\rm L}}}{\sqrt{2}}\varLambda_{21}^{x}.
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Now we will deduce the equations:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\partial_{t}\mathcal{\hat{O}}={\rm i}\left[H,\mathcal{\hat{O}}\right]+\mathcal{L}^{*}\left[\mathcal{\hat{O}}\right].
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
1) 
\begin_inset Formula $a_{{\rm L}}$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\partial_{t}a_{{\rm L}} & ={\rm i}\left[H,a_{{\rm L}}\right]+\mathcal{L}^{*}\left[a_{{\rm L}}\right]+{\rm Noise},\\
\left[H,a_{{\rm L}}\right] & =\left[g_{{\rm L}}a_{{\rm L}}^{\dagger}\left|1\right\rangle \!\!\left\langle 2\right|_{i}-\Delta_{{\rm L}}a_{{\rm L}}^{\dagger}a_{{\rm L}},a_{{\rm L}}\right]\\
 & =-g_{{\rm L}}\left|1\right\rangle \!\!\left\langle 2\right|_{i}+\Delta_{{\rm L}}a_{{\rm L}},\\
\mathcal{L}^{*}\left[a_{{\rm L}}\right] & =\frac{\kappa_{{\rm L}}}{2}\left(2a_{{\rm L}}^{\dagger}a_{{\rm L}}a_{{\rm L}}-a_{{\rm L}}^{\dagger}a_{{\rm L}}a_{{\rm L}}-a_{{\rm L}}a_{{\rm L}}^{\dagger}a_{{\rm L}}\right)\\
 & =\frac{\kappa_{{\rm L}}}{2}\left(a_{{\rm L}}^{\dagger}a_{{\rm L}}a_{{\rm L}}-a_{{\rm L}}a_{{\rm L}}^{\dagger}a_{{\rm L}}\right)\\
 & =-\frac{\kappa_{{\rm L}}}{2}a_{{\rm L}},\\
\partial_{t}a_{{\rm L}} & ={\rm i}\left(-g_{{\rm L}}\left|1\right\rangle \!\!\left\langle 2\right|_{i}+\Delta_{{\rm L}}a_{{\rm L}}\right)-\frac{\kappa_{{\rm L}}}{2}a_{{\rm L}}+\sqrt{\kappa_{{\rm L}}/2}\left({\rm d}W_{1{\rm L}}+{\rm i}{\rm d}W_{2{\rm L}}\right)\\
 & ={\rm i}\left(-\frac{g_{{\rm L}}}{\sqrt{2}}\left(\varLambda_{21,i}^{x}+{\rm i}\varLambda_{21,i}^{y}\right)+\Delta_{{\rm L}}a_{{\rm L}}\right)-\frac{\kappa_{{\rm L}}}{2}a_{{\rm L}}+\sqrt{\kappa_{{\rm L}}/2}\left({\rm d}W_{1{\rm L}}+{\rm i}{\rm d}W_{2{\rm L}}\right).
\end{align}

\end_inset


\end_layout

\begin_layout Standard
2) 
\begin_inset Formula $a_{{\rm R}}$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\partial_{t}a_{{\rm {\rm R}}} & ={\rm i}\left[H,a_{{\rm {\rm R}}}\right]+\mathcal{L}^{*}\left[a_{{\rm {\rm R}}}\right]+{\rm Noise},\\
\left[H,a_{{\rm R}}\right] & =\left[g_{{\rm R}}a_{{\rm R}}^{\dagger}\left|0\right\rangle \!\!\left\langle 3\right|_{i}-\Delta_{{\rm R}}a_{{\rm R}}^{\dagger}a_{{\rm R}},a_{{\rm R}}\right]\\
 & =-g_{{\rm R}}\left|0\right\rangle \!\!\left\langle 3\right|_{i}+\Delta_{{\rm R}}a_{{\rm R}},\\
\mathcal{L}^{*}\left[a_{{\rm {\rm R}}}\right] & =\frac{\kappa_{{\rm {\rm R}}}}{2}\left(2a_{{\rm {\rm {\rm R}}}}^{\dagger}a_{{\rm {\rm R}}}a_{{\rm {\rm {\rm R}}}}-a_{{\rm {\rm {\rm R}}}}^{\dagger}a_{{\rm {\rm {\rm R}}}}a_{{\rm {\rm R}}}-a_{{\rm {\rm R}}}a_{{\rm {\rm {\rm R}}}}^{\dagger}a_{{\rm {\rm {\rm R}}}}\right)\\
 & =\frac{\kappa_{{\rm {\rm {\rm R}}}}}{2}\left(a_{{\rm {\rm {\rm R}}}}^{\dagger}a_{{\rm {\rm {\rm R}}}}a_{{\rm {\rm R}}}-a_{{\rm {\rm R}}}a_{{\rm {\rm R}}}^{\dagger}a_{{\rm {\rm {\rm R}}}}\right)\\
 & =-\frac{\kappa_{{\rm {\rm {\rm R}}}}}{2}a_{{\rm {\rm {\rm R}}}},\\
\partial_{t}a_{{\rm R}} & ={\rm i}\left(-g_{{\rm R}}\frac{\varLambda_{30,i}^{x}+{\rm i}\varLambda_{30,i}^{y}}{\sqrt{2}}+\Delta_{{\rm R}}a_{{\rm R}}\right)-\frac{\kappa_{{\rm {\rm {\rm R}}}}}{2}a_{{\rm {\rm {\rm R}}}}+\sqrt{\kappa_{{\rm R}}/2}\left({\rm d}W_{1{\rm R}}+{\rm i}{\rm d}W_{2{\rm R}}\right)\\
 & ={\rm i}\left(-\frac{g_{{\rm R}}}{\sqrt{2}}\left(\varLambda_{30,i}^{x}+{\rm i}\varLambda_{30,i}^{y}\right)+\Delta_{{\rm R}}a_{{\rm R}}\right)-\frac{\kappa_{{\rm {\rm {\rm R}}}}}{2}a_{{\rm {\rm {\rm R}}}}+\sqrt{\kappa_{{\rm R}}/2}\left({\rm d}W_{1{\rm R}}+{\rm i}{\rm d}W_{2{\rm R}}\right).
\end{align}

\end_inset


\end_layout

\begin_layout Standard
3) 
\begin_inset Formula $\varLambda_{10,i}^{x}=\frac{1}{\sqrt{2}}\left(\left|1\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 1\right|_{i}\right)$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\left[a_{{\rm R}}\left|3\right\rangle \!\!\left\langle 0\right|_{i},\varLambda_{10,i}^{x}\right] & =\left[a_{{\rm R}}\left|3\right\rangle \!\!\left\langle 0\right|_{i},\varLambda_{10,i}^{x}\right]\\
 & =a_{{\rm R}}\left[\left|3\right\rangle \!\!\left\langle 0\right|_{i},\frac{1}{\sqrt{2}}\left(\left|1\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 1\right|_{i}\right)\right]\\
 & =\frac{a_{{\rm R}}}{\sqrt{2}}\left(\left[\left|3\right\rangle \!\!\left\langle 0\right|_{i},\left|1\right\rangle \!\!\left\langle 0\right|_{i}\right]+\left[\left|3\right\rangle \!\!\left\langle 0\right|_{i},\left|0\right\rangle \!\!\left\langle 1\right|_{i}\right]\right)\\
 & =\frac{a_{{\rm R}}}{\sqrt{2}}\left(\left|3\right\rangle \!\!\left\langle 1\right|_{i}\right)\\
 & =\frac{a_{{\rm R}}}{\sqrt{2}}\left(\frac{\varLambda_{31,i}^{x}-{\rm i}\varLambda_{31,i}^{y}}{\sqrt{2}}\right)\\
 & =\frac{a_{{\rm R}}}{2}\left(\varLambda_{31,i}^{x}-{\rm i}\varLambda_{31,i}^{y}\right),\\
\left[a_{{\rm R}}^{\dagger}\left|0\right\rangle \!\!\left\langle 3\right|_{i},\varLambda_{10,i}^{x}\right] & =a_{{\rm R}}^{\dagger}\left[\left|0\right\rangle \!\!\left\langle 3\right|_{i},\frac{1}{\sqrt{2}}\left(\left|1\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 1\right|_{i}\right)\right]\\
 & =\frac{a_{{\rm R}}^{\dagger}}{\sqrt{2}}\left(\left[\left|0\right\rangle \!\!\left\langle 3\right|_{i},\left|1\right\rangle \!\!\left\langle 0\right|_{i}\right]+\left[\left|0\right\rangle \!\!\left\langle 3\right|_{i},\left|0\right\rangle \!\!\left\langle 1\right|_{i}\right]\right)\\
 & =\frac{a_{{\rm R}}^{\dagger}}{\sqrt{2}}\left(-\left|1\right\rangle \!\!\left\langle 3\right|_{i}\right)\\
 & =-\frac{a_{{\rm R}}^{\dagger}}{2}\left(\varLambda_{31,i}^{x}+{\rm i}\varLambda_{31,i}^{y}\right)\\
\left[\frac{\Omega_{{\rm R}}}{\sqrt{2}}\varLambda_{30}^{x},\varLambda_{10,i}^{x}\right] & =\frac{\Omega_{{\rm R}}}{\sqrt{2}}\left[\varLambda_{30}^{x},\varLambda_{10,i}^{x}\right]\\
 & =\frac{\Omega_{{\rm R}}}{\sqrt{2}}\left[\frac{1}{\sqrt{2}}\left(\left|3\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 3\right|_{i}\right),\frac{1}{\sqrt{2}}\left(\left|1\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 1\right|_{i}\right)\right]\\
 & =\frac{\Omega_{{\rm R}}}{2\sqrt{2}}\left[\left|3\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 3\right|_{i},\left|1\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 1\right|_{i}\right]\\
 & =\frac{\Omega_{{\rm R}}}{2\sqrt{2}}\left(\left[\left|3\right\rangle \!\!\left\langle 0\right|_{i},\left|1\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 1\right|_{i}\right]+\left[\left|0\right\rangle \!\!\left\langle 3\right|_{i},\left|1\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 1\right|_{i}\right]\right)\\
 & =\frac{\Omega_{{\rm R}}}{2\sqrt{2}}\left(\left|3\right\rangle \!\!\left\langle 1\right|_{i}-\left|1\right\rangle \!\!\left\langle 3\right|_{i}\right)\\
 & =-\frac{\Omega_{{\rm R}}}{2\sqrt{2}}\left(\left|1\right\rangle \!\!\left\langle 3\right|_{i}-\left|3\right\rangle \!\!\left\langle 1\right|_{i}\right)\\
 & =-\frac{\Omega_{{\rm R}}}{2\sqrt{2}}\sqrt{2}{\rm i}\varLambda_{31,i}^{y}\\
 & =-\frac{\Omega_{{\rm R}}}{2}{\rm i}\varLambda_{31,i}^{y},\\
\left[a_{{\rm L}}\left|2\right\rangle \!\!\left\langle 1\right|_{i},\varLambda_{10,i}^{x}\right] & =\left[a_{{\rm L}}\left|2\right\rangle \!\!\left\langle 1\right|_{i},\frac{1}{\sqrt{2}}\left(\left|1\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 1\right|_{i}\right)\right]\\
 & =\frac{a_{{\rm L}}}{\sqrt{2}}\left[\left|2\right\rangle \!\!\left\langle 1\right|_{i},\left|1\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 1\right|_{i}\right]\\
 & =\frac{a_{{\rm L}}}{\sqrt{2}}\left|2\right\rangle \!\!\left\langle 0\right|_{i}\\
 & =\frac{a_{{\rm L}}}{\sqrt{2}}\frac{\varLambda_{20,i}^{x}-{\rm i}\varLambda_{20,i}^{y}}{\sqrt{2}}\\
 & =\frac{a_{{\rm L}}}{2}\left(\varLambda_{20,i}^{x}-{\rm i}\varLambda_{20,i}^{y}\right),\\
\left[a_{{\rm L}}^{\dagger}\left|1\right\rangle \!\!\left\langle 2\right|_{i},\varLambda_{10,i}^{x}\right] & =\left[a_{{\rm L}}^{\dagger}\left|1\right\rangle \!\!\left\langle 2\right|_{i},\frac{1}{\sqrt{2}}\left(\left|1\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 1\right|_{i}\right)\right]\\
 & =\frac{a_{{\rm L}}^{\dagger}}{\sqrt{2}}\left[\left|1\right\rangle \!\!\left\langle 2\right|_{i},\left|1\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 1\right|_{i}\right]
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
 & =\frac{a_{{\rm L}}^{\dagger}}{\sqrt{2}}\left(-\left|0\right\rangle \!\!\left\langle 2\right|_{i}\right)\\
 & =-\frac{a_{{\rm L}}^{\dagger}}{\sqrt{2}}\frac{\varLambda_{20,i}^{x}+{\rm i}\varLambda_{20,i}^{y}}{\sqrt{2}}\\
 & =-\frac{a_{{\rm L}}^{\dagger}}{2}\left(\varLambda_{20,i}^{x}+{\rm i}\varLambda_{20,i}^{y}\right)\\
\left[\frac{\Omega_{{\rm L}}}{\sqrt{2}}\varLambda_{21}^{x},\varLambda_{10,i}^{x}\right] & =\frac{\Omega_{{\rm L}}}{2\sqrt{2}}\left[\varLambda_{21}^{x},\varLambda_{10,i}^{x}\right]\\
