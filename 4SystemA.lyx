\begin_layout Standard
\begin_inset Formula 
\begin{align}
H & =\chi_{L}L^{+}L^{-}+\chi_{R}R^{+}R^{-}+\Omega_{L}L_{x}+\Omega_{R}R_{x},\\
\mathcal{L}_{i}\left[\rho\right] & =L_{i}\rho L_{i}^{\dagger}-\frac{1}{2}\left\{ L_{i}^{\dagger}L_{i},\rho\right\} ,\\
\mathcal{L}_{i}^{*}\left[\mathcal{O}\right] & =L_{i}^{\dagger}\mathcal{O}L_{i}-\frac{1}{2}\left\{ L_{i}^{\dagger}L_{i},\mathcal{O}\right\} ,\\
L_{i} & \Rightarrow\sqrt{\Gamma_{i}}L_{i}^{-},\\
L_{1}^{-} & =L^{-},\\
L_{2}^{-} & =R^{-}.
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Expanding:
\end_layout

\begin_layout Standard

\size footnotesize
\begin_inset Formula 
\begin{align}
L^{+}L^{-} & =\sum_{ii^{\prime}}\left|e_{-1/2}\right\rangle _{i}\!\!\left\langle g_{1/2}\right|_{i}\left|g_{1/2}\right\rangle _{i^{\prime}}\!\!\left\langle e_{-1/2}\right|_{i^{\prime}},\\
 & =\sum_{ii^{\prime}}\left|2\right\rangle _{i}\!\!\left\langle 1\right|_{i}\left|1\right\rangle _{i^{\prime}}\!\!\left\langle 2\right|_{i^{\prime}},\\
 & =\sum_{i}\left|2\right\rangle _{i}\!\!\left\langle 2\right|_{i},\\
R^{+}R^{-} & =\sum_{ii^{\prime}}\left|e_{1/2}\right\rangle _{i}\!\!\left\langle g_{-1/2}\right|_{i}\left|g_{-1/2}\right\rangle _{i^{\prime}}\!\!\left\langle e_{1/2}\right|_{i^{\prime}},\\
 & =\sum_{ii^{\prime}}\left|3\right\rangle _{i}\!\!\left\langle 0\right|_{i}\left|0\right\rangle _{i^{\prime}}\!\!\left\langle 3\right|_{i^{\prime}},\\
 & =\sum_{i}\left|3\right\rangle _{i}\!\!\left\langle 3\right|_{i},\\
H & =\chi_{L}\sum_{i}\left|2\right\rangle _{i}\!\!\left\langle 2\right|_{i}+\chi_{R}\sum_{i}\left|3\right\rangle _{i}\!\!\left\langle 3\right|_{i}+\frac{\Omega_{L}}{2}\sum_{i}\left(\left|2\right\rangle \!\!\left\langle 1\right|_{i}+\left|1\right\rangle \!\!\left\langle 2\right|_{i}\right)\\
 & \,\,\,\,\,+\frac{\Omega_{R}}{2}\sum_{i}\left(\left|3\right\rangle \!\!\left\langle 0\right|_{i}+\left|0\right\rangle \!\!\left\langle 3\right|_{i}\right)\\
 & =\sum_{i}\left(\chi_{L}\left|2\right\rangle _{i}\!\!\left\langle 2\right|_{i}+\chi_{R}\left|3\right\rangle _{i}\!\!\left\langle 3\right|_{i}+\frac{\Omega_{L}}{2}\left(\left|2\right\rangle _{i}\!\!\left\langle 1\right|_{i}+\left|1\right\rangle _{i}\!\!\left\langle 2\right|_{i}\right)+\frac{\Omega_{R}}{2}\left(\left|3\right\rangle _{i}\!\!\left\langle 0\right|_{i}+\left|0\right\rangle _{i}\!\!\left\langle 3\right|_{i}\right)\right).
\end{align}

\end_inset


\end_layout

