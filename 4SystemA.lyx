 & \,\,\,\,\,+g_{{\rm L}}\sum_{i}\left(a_{{\rm L}}\left|2\right\rangle \!\!\left\langle 1\right|_{i}+a_{{\rm L}}^{\dagger}\left|1\right\rangle \!\!\left\langle 2\right|_{i}\right)-\Delta_{{\rm L}}a_{{\rm L}}^{\dagger}a_{{\rm L}}+\frac{\Omega_{{\rm L}}}{2}\sum_{i}\left(\left|2\right\rangle \!\!\left\langle 1\right|_{i}+\left|1\right\rangle \!\!\left\langle 2\right|_{i}\right),\\
\left[\left|c\right\rangle \!\!\left\langle d\right|_{i},\left|f\right\rangle \!\!\left\langle h\right|_{i}\right] & =\left|c\right\rangle \!\!\left\langle d\right|_{i}\left|f\right\rangle \!\!\left\langle h\right|_{i}-\left|f\right\rangle \!\!\left\langle h\right|_{i}\left|c\right\rangle \!\!\left\langle d\right|_{i}\\
 & =\delta_{df}\left|c\right\rangle \!\!\left\langle h\right|_{i}-\delta_{ch}\left|f\right\rangle \!\!\left\langle d\right|_{i},\\
\left[\left|3\right\rangle \!\!\left\langle 0\right|_{i},\left|f\right\rangle \!\!\left\langle h\right|_{i}\right] & =\delta_{0f}\left|3\right\rangle \!\!\left\langle h\right|_{i}-\delta_{3h}\left|f\right\rangle \!\!\left\langle 0\right|_{i},\\
\left[\left|0\right\rangle \!\!\left\langle 3\right|_{i},\left|f\right\rangle \!\!\left\langle h\right|_{i}\right] & =\delta_{3f}\left|0\right\rangle \!\!\left\langle h\right|_{i}-\delta_{0h}\left|f\right\rangle \!\!\left\langle 3\right|_{i},\\
\left[\sigma_{30}^{x},\left|f\right\rangle \!\!\left\langle h\right|_{i}\right] & =\delta_{0f}\left|3\right\rangle \!\!\left\langle h\right|_{i}-\delta_{3h}\left|f\right\rangle \!\!\left\langle 0\right|_{i}+\delta_{3f}\left|0\right\rangle \!\!\left\langle h\right|_{i}-\delta_{0h}\left|f\right\rangle \!\!\left\langle 3\right|_{i}\\
 & =\delta_{0f}\left|3\right\rangle \!\!\left\langle h\right|_{i}-\delta_{0h}\left|f\right\rangle \!\!\left\langle 3\right|_{i}+\delta_{3f}\left|0\right\rangle \!\!\left\langle h\right|_{i}-\delta_{3h}\left|f\right\rangle \!\!\left\langle 0\right|_{i},\\
\left[\left|2\right\rangle \!\!\left\langle 1\right|_{i},\left|f\right\rangle \!\!\left\langle h\right|_{i}\right] & =\delta_{1f}\left|2\right\rangle \!\!\left\langle h\right|_{i}-\delta_{2h}\left|f\right\rangle \!\!\left\langle 1\right|_{i},\\
\left[\left|1\right\rangle \!\!\left\langle 2\right|_{i},\left|f\right\rangle \!\!\left\langle h\right|_{i}\right] & =\delta_{2f}\left|1\right\rangle \!\!\left\langle h\right|_{i}-\delta_{1h}\left|f\right\rangle \!\!\left\langle 2\right|_{i},\\
\left[\sigma_{21}^{x},\left|f\right\rangle \!\!\left\langle h\right|_{i}\right] & =\delta_{1f}\left|2\right\rangle \!\!\left\langle h\right|_{i}-\delta_{2h}\left|f\right\rangle \!\!\left\langle 1\right|_{i}+\delta_{2f}\left|1\right\rangle \!\!\left\langle h\right|_{i}-\delta_{1h}\left|f\right\rangle \!\!\left\langle 2\right|_{i}\\
 & =\delta_{1f}\left|2\right\rangle \!\!\left\langle h\right|_{i}-\delta_{1h}\left|f\right\rangle \!\!\left\langle 2\right|_{i}+\delta_{2f}\left|1\right\rangle \!\!\left\langle h\right|_{i}-\delta_{2h}\left|f\right\rangle \!\!\left\langle 1\right|_{i},\\
\left[H,a_{{\rm \gamma}}\right] & =-g_{{\rm \gamma}}\sum_{i}\left|1\right\rangle \!\!\left\langle 2\right|_{i}+\Delta_{\gamma}a_{\gamma},\\
\mathcal{L}_{\gamma}^{*}\left[a_{\gamma}\right] & =2a_{\gamma}^{\dagger}a_{\gamma}a_{\gamma}-a_{\gamma}^{\dagger}a_{\gamma}a_{\gamma}-a_{\gamma}a_{\gamma}^{\dagger}a_{\gamma}\\
 & =\left(a_{\gamma}^{\dagger}a_{\gamma}-a_{\gamma}a_{\gamma}^{\dagger}\right)a_{\gamma}\\
 & =-a_{\gamma}.
\end{align}

